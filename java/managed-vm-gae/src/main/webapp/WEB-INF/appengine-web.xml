    <vm>true</vm>

    <env-variables>
      <env-var name="BIGTABLE_PROJECT" value="${bigtable.projectID}" />
      <env-var name="BIGTABLE_CLUSTER" value="${bigtable.clusterID}" />
      <env-var name="BIGTABLE_ZONE" value="${bigtable.zone}" />

      <env-var name="ALPN_ENABLE" value="true" />
      
    </env-variables>
    <system-properties>
