<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <threadsafe>true</threadsafe>    
    <vm>true</vm>

    <env-variables>
      <env-var name="BIGTABLE_PROJECT" value="YOUR_PROJECT_ID" />
      <env-var name="BIGTABLE_CLUSTER" value="YOUR_CLUSTER_ID" />
      <env-var name="BIGTABLE_ZONE" value="us-central1-b" />

      <!-- env-var name="GOOGLE_APPLICATION_CREDENTIALS" value="/app/WEB-INF/client-secret.json" -->

      <env-var name="ALPN_ENABLE" value="true" />
    </env-variables>

</appengine-web-app>
